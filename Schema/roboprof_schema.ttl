@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix vivo: <http://vivoweb.org/ontology/core#> .
@prefix ex: <http://example.org/ns#> .
@prefix dbo: <http://dbpedia.org/ontology/> .
@prefix dbr: <http://dbpedia.org/resource/> .

# Classes
ex:University a rdfs:Class ;
    rdfs:label "University"@en ;
    rdfs:comment "An institution of higher education and research."@en .
ex:Course a rdfs:Class ;
    rdfs:label "Course"@en ;
    rdfs:comment "A unit of teaching that typically lasts one academic term, is led by one or more instructors, and has a fixed roster of students."@en .
ex:Lecture a rdfs:Class ;
    rdfs:label "Lecture"@en ;
    rdfs:comment "A lecture is a specific instance of instruction within a course."@en .
ex:Topic a rdfs:Class ;
    rdfs:label "Topic"@en ;
    rdfs:comment "A specific subject matter covered within the academic content of a course."@en .
ex:Student a rdfs:Class ;
    rdfs:label "Student"@en ;
    rdfs:subClassOf foaf:Person ;
    rdfs:comment "An individual who is studying at a university or other place of higher education."@en .

# Properties
## General properties
ex:hasName a rdf:Property ;
    rdfs:label "has Name"@en ;
    rdfs:domain rdfs:Resource ;
    rdfs:range rdfs:Literal .

ex:hasLink a rdf:Property ;
    rdfs:label "has Link"@en ;
    rdfs:domain rdfs:Resource ;
    rdfs:range rdfs:Literal .

## University to Courses
ex:offersCourse a rdf:Property ;
    rdfs:label "offers Course"@en ;
    rdfs:domain ex:University ;
    rdfs:range ex:Course .

## Course properties
ex:courseSubject a rdf:Property ;
    rdfs:label "course Subject"@en ;
    rdfs:domain ex:Course ;
    rdfs:range rdfs:Literal .

ex:courseNumber a rdf:Property ;
    rdfs:label "course Number"@en ;
    rdfs:domain ex:Course ;
    rdfs:range rdfs:Literal .

ex:courseCredits a rdf:Property ;
    rdfs:label "course Credits"@en ;
    rdfs:domain ex:Course ;
    rdfs:range rdfs:Literal .

ex:courseDescription a rdf:Property ;
    rdfs:label "course Description"@en ;
    rdfs:domain ex:Course ;
    rdfs:range rdfs:Literal .

ex:courseOutlineLink a rdf:Property ;
    rdfs:label "course Outline Link"@en ;
    rdfs:domain ex:Course ;
    rdfs:range rdfs:Literal .

# Lecture properties
ex:lectureNumber a rdf:Property ;
    rdfs:label "Lecture Number"@en ;
    rdfs:domain ex:Lecture ;
    rdfs:range rdfs:Literal .

ex:lectureName a rdf:Property ;
    rdfs:label "Lecture Name"@en ;
    rdfs:domain ex:Lecture ;
    rdfs:range rdfs:Literal .

ex:lectureContentLink a rdf:Property ;
    rdfs:label "Lecture Content Link"@en ;
    rdfs:domain ex:Lecture ;
    rdfs:range rdfs:Literal .

rdfs:seeAlso a rdf:Property ;
    rdfs:label "See Also"@en ;
    rdfs:comment "A link to more information about the subject."@en .

# Subclass hierarchy for lecture content
ex:LectureContent a rdfs:Class ;
    rdfs:label "Lecture Content"@en ;
    rdfs:comment "The content provided in a lecture."@en .

ex:Slides a rdfs:Class ;
    rdfs:label "Slides"@en ;
    rdfs:comment "Presentation slides used in the lecture."@en ;
    rdfs:subClassOf ex:LectureContent .

ex:Worksheets a rdfs:Class ;
    rdfs:label "Worksheets"@en ;
    rdfs:comment "Worksheets provided for the lecture."@en ;
    rdfs:subClassOf ex:LectureContent .

ex:Readings a rdfs:Class ;
    rdfs:label "Readings"@en ;
    rdfs:comment "Reading materials recommended for the lecture."@en ;
    rdfs:subClassOf ex:LectureContent .

ex:OtherMaterial a rdfs:Class ;
    rdfs:label "Other Material"@en ;
    rdfs:comment "Other types of materials provided for the lecture, such as videos or images."@en ;
    rdfs:subClassOf ex:LectureContent .

## Topic properties
ex:topicName a rdf:Property ;
    rdfs:label "topic Name"@en ;
    rdfs:domain ex:Topic ;
    rdfs:range rdfs:Literal .

ex:topicProvenance a rdf:Property ;
    rdfs:label "topic Provenance"@en ;
    rdfs:domain ex:Topic ;
    rdfs:range rdfs:Literal .

ex:topicLink a rdf:Property ;
    rdfs:label "topic Link"@en ;
    rdfs:domain ex:Topic ;
    rdfs:range rdfs:Literal .

## Student properties
ex:studentID a rdf:Property ;
    rdfs:label "student ID"@en ;
    rdfs:domain ex:Student ;
    rdfs:range rdfs:Literal .

foaf:mbox a rdf:Property ;
    rdfs:label "email"@en ;
    rdfs:domain ex:Student ;
    rdfs:range rdfs:Literal .
  
ex:completedCourse a rdf:Property ;
    rdfs:label "completed Course"@en ;
    rdfs:domain ex:Student ;
    rdfs:range ex:Course .

ex:courseGrade a rdf:Property ;
    rdfs:label "course Grade"@en ;
    rdfs:domain [a rdfs:Class ; rdfs:label "Completed Course"@en ; rdfs:comment "Represents a course completed by a student, possibly retaken for a different grade."] ;
    rdfs:range rdfs:Literal .

ex:hasCompetency a rdf:Property ;
    rdfs:label "has Competency"@en ;
    rdfs:domain ex:Student ;
    rdfs:range ex:Topic .